<%# cache [action_name, I18n.locale, site, "photos", "page", params[:page] || 1, parent.photos_cached_key] do %>
	<%#= render_collection_header %>
  <article class="container">
    <div class="row">
      <div class="col-xs-12">
        <div class="page-header">
          <h1><%= parent.title %></h1>
        </div>
        <%= render_copy_text(parent.body) %>
      </div> 
    </div>  
  </article>
  <section class="container">
  	<div class="row">
      <%= render collection %>
    </div>
  </section> 
<%# end %>

